{
  "name": "exclude-wx-dom-loader",
  "version": "0.0.1",
  "description": "a tool for develop miniprogram with vue",
  "main": "src/index.js",
  "miniprogram": "src",
  "scripts": {
    "test": "jest --bail",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
    "coverage": "jest --coverage --bail",
    "codecov": "jest --coverage && codecov"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wzs28150/exclude-wx-dom-loader.git"
  },
  "author": "wzs28150",
  "license": "MIT",
  "devDependencies": {
    "jest": "^24.8.0"
  }
}